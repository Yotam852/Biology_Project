@article{landsberg_increased_2009,
  title = {Increased {{Cell Bond Tension Governs Cell Sorting}} at the {{Drosophila Anteroposterior Compartment Boundary}}},
  author = {Landsberg, Katharina P. and Farhadifar, Reza and Ranft, Jonas and Umetsu, Daiki and Widmann, Thomas J. and Bittig, Thomas and Said, Amani and Jülicher, Frank and Dahmann, Christian},
  date = {2009},
  journaltitle = {Current Biology},
  volume = {19},
