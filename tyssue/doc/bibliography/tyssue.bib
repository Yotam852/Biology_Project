@article{tamulonis_cell-based_2011,
  title = {A Cell-Based Model of {{Nematostella}} Vectensis Gastrulation Including Bottle Cell Formation, Invagination and Zippering},
  author = {Tamulonis, Carlos and Postma, Marten and Marlow, Heather Q. and Magie, Craig R. and de Jong, Johann and Kaandorp, Jaap},
  options = {useprefix=true},
  date = {2011},
  journaltitle = {Developmental Biology},
  volume = {351},
